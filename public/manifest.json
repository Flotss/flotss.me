{
  "author": "Flotss",
  "name": "Flotss.me Portfolio Website",
  "short_name": "Flotss.me",
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "version": "1.0",
  "commands": [
    {
      "name": "open",
      "description": "Opens the website",
      "url": "/"
    },
    {
      "name": "projects",
      "description": "Opens the projects page",
      "url": "/projects"
    },
    {
      "name": "projects/[name]",
      "description": "Opens the project page for the project with the name [name]",
      "url": "/projects/[name]"
    },
    {
      "name": "contact",
      "description": "Opens the contact page",
      "url": "/contact"
    },
    {
      "name": "about",
      "description": "Opens the about page",
      "url": "/about"
    }
  ]
}
